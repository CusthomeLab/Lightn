{
  "name": "lightn",
  "version": "0.1.0",
  "private": true,
  "author": "Custhome",
  "description": "Custhome picture / modelisation optimisation with squoosh",
  "main": "./public/electron.js",
  "homepage": "./",
  "dependencies": {
    "@squoosh/lib": "*",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "antd": "^4.16.13",
    "electron-is-dev": "^2.0.0",
    "electron-log": "^4.4.1",
    "electron-updater": "^4.3.9",
    "mobx": "^6.3.3",
    "mobx-react-lite": "^3.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "styled-components": "^5.3.1",
    "walk": "^2.3.14",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron:dev": "concurrently -k \"cross-env BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electronmon .\"",
    "electron:pack:test": "yarn build && DEBUG=electron-builder electron-builder --dir",
    "electron:pack": "yarn build && electron-builder --x64 --macos --win",
    "electron:release": "yarn build && electron-builder --x64 --macos --win --publish onTag"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [],
    "development": []
  },
  "devDependencies": {
    "concurrently": "^6.2.1",
    "cross-env": "^7.0.3",
    "electron": "^14.0.1",
    "electron-builder": "^22.11.7",
    "electronmon": "^2.0.2",
    "wait-on": "^6.0.0"
  }
}
